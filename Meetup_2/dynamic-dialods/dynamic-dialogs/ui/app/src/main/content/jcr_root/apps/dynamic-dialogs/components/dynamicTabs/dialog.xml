                            jcr:primaryType="nt:unstructured"
                            valid="function( field){console.log('valid');}"
                            loadcontent="function(field,record,path){console.log('loadcontent');}"
                            invalid="function( field, msg){console.log('invalid');}"
                            beforeloadcontent="function( selection, value, isChecked){console.log('beforeloadcontent');}"
                            selectionchanged="function( selection, value, isChecked){console.log('selectionchanged');}"
                            activate="function(panel){console.log('activate');}"
                            add="function(container, component, index){console.log('add');}"
                            added="function( component, container,index ){console.log('added');}"
                            afterlayout="function( container, containerLayout){console.log('afterlayout');}"
                            afterrender="function( component){console.log('afterrender');}"
                            beforeadd="function( container, component,index ){console.log('beforeadd');}"
                            beforeclose="function( panel){console.log('beforeclose');}"
                            beforecollapse="function(panel,animate ){console.log('beforecollapse');}"
                            beforedestroy="function( component){console.log('beforedestroy');}"
                            beforeexpand="function( panel, animate ){console.log('beforeexpand');}"
                            beforehide="function( component){console.log('beforehide');}"
                            beforeremove="function( container, component ){console.log('beforeremove');}"
                            beforerender="function( component){console.log('beforerender');}"
                            beforeshow="function( component ){console.log('beforeshow');}"
                            beforestaterestore="function( component, state ){console.log('beforestaterestore');}"
                            beforestatesave="function( component, state ){console.log('beforestatesave');}"
                            bodyresize="function( panel, width, height ){console.log('bodyresize');}"
                            close="function( panel){console.log('close');}"
                            collapse="function( panel ){console.log('collapse');}"
                            deactivate="function(panel){console.log('deactivate');}"
                            destroy="function( component ){console.log('destroy');}"
                            disable="function( component ){console.log('disable');}"
                            enable="function( component ){console.log('enable');}"
                            expand="function(panel){console.log('expand');}"
                            hide="function(component){console.log('hide');}"
                            iconchange="function( panel, newIcon, oldIcon ){console.log('iconchange');}"
                            move="function( component, x, y ){console.log('move');}"
                            remove="function(container, component){console.log('remove');}"
                            removed="function(component ,ownerCt){console.log('removed');}"
                            render="function(component){console.log('render');}"
                            resize="function( component, adjWidth, adjHeight, rawWidth, rawHeight ){console.log('resize');}"
                            show="function(component){console.log('show');}"
                            staterestore="function(component,state){console.log('staterestore');}"
                            statesave="function(component,state){console.log('statesave');}"
                            titlechange="function(panel, title){console.log('titlechange');}"/>
                        <options jcr:primaryType="cq:WidgetCollection">
                            <v1
                                jcr:primaryType="nt:unstructured"
                                text="Show Tabs X"
                                value="v_1"/>
                            <v2
